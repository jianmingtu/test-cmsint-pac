<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema targetNamespace="ICON2.Source.HealthServiceRequest.ws.provider:HSR"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="ICON2.Source.HealthServiceRequest.ws.provider:HSR">

    <xsd:element name="publishHSR">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="publishHSRDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:healthServiceOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="healthServiceOuter">
        <xsd:sequence>
            <xsd:element name="HealthService" nillable="true" type="tns:healthService"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="healthService">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="HealthServiceRequest" nillable="true" minOccurs="0" maxOccurs="unbounded" type="tns:healthServiceRequest"/>
            <xsd:element name="Row" nillable="true" type="tns:row"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="healthServiceRequest">
        <xsd:sequence>
            <xsd:element name="hsrId" nillable="true" type="xsd:string"/>
            <xsd:element name="pacID" nillable="true" type="xsd:string"/>
            <xsd:element name="location" nillable="true" type="xsd:string"/>
            <xsd:element name="requestDate" nillable="true" type="xsd:string"/>
            <xsd:element name="healthRequest" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="row">
        <xsd:sequence>
            <xsd:element name="start" nillable="true" type="xsd:string"/>
            <xsd:element name="end" nillable="true" type="xsd:string"/>
            <xsd:element name="total" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="publishHSRResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="publishHSRResponseDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:healthServiceOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="Error">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Reason" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getHSRCount">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getHSRCountDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:HSRCountOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="HSRCountOuter">
        <xsd:sequence>
            <xsd:element name="HealthServiceCount" nillable="true" type="tns:HSRCount"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="HSRCount">
        <xsd:sequence>
            <xsd:element name="csNum" nillable="true" type="xsd:string"/>
            <xsd:element name="count" nillable="true" type="xsd:string"/>
            <xsd:element name="max" nillable="true" type="xsd:string"/>
            <xsd:element name="hsrId" nillable="true" type="xsd:string"/>
            <xsd:element name="xmitOkay" nillable="true" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="userTokenOuter">
        <xsd:sequence>
            <xsd:element name="UserToken" nillable="true" type="tns:userToken"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="userToken">
        <xsd:sequence>
            <xsd:element name="RemoteClientBrowserType" type="xsd:string"/>
            <xsd:element name="RemoteClientHostName" type="xsd:string"/>
            <xsd:element name="RemoteClientIPAddress" type="xsd:string"/>
            <xsd:element name="UserIdentifier" type="xsd:string"/>
            <xsd:element name="AuthoritativePartyIdentifier" type="xsd:string"/>
            <xsd:element name="BiometricsSignature" type="xsd:string"/>
            <xsd:element name="CSNumber" type="xsd:string"/>
            <xsd:element name="SiteMinderSessionID" type="xsd:string"/>
            <xsd:element name="SiteMinderTransactionID" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="getHSRCountResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true"  type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true"  type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getHSRCountResponseDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:HSRCountOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getHealthServiceRequestHistory">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getHealthServiceRequestHistoryDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:healthServiceOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getHealthServiceRequestHistoryResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="xsd:string"/>
                <xsd:element name="UserTokenString" nillable="true" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getHealthServiceRequestHistoryResponseDocument">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="XMLString" nillable="true" type="tns:healthServiceOuter"/>
                <xsd:element name="UserTokenString" nillable="true" type="tns:userTokenOuter"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

</xsd:schema>

